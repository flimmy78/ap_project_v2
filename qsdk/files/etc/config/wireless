config wifi-device 'wifi0'
        option type 'qcawifi'
        option hwmode '11ng'
        option htmode 'HT20'
        option channel '11'
        option country 'US'
        option disabled '0'
        option txpower '22'

config wifi-iface
	option device 'wifi0'
	option network 'v2'
	option mode 'ap'
	option hidden '1'
	option ssid 'config.9797168.com'
	option encryption 'psk2+tkip'
	option key 'Fayxzx00'

config wifi-iface
         option device 'wifi0'
         option network 'v1'
         option mode 'ap'
         option ssid '9797168.com'
         option encryption 'none'

config wifi-device 'wifi1'
	option type 'qcawifi'
	option hwmode '11na'
	option htmode 'HT20'
	option country 'US'
	option channel '36'
	option txpower '19'
	option disabled '0'
	
config wifi-iface
	option device 'wifi1'
	option network 'v1'
	option mode 'ap'
	option ssid '9797168.com'
	option encryption 'none'

